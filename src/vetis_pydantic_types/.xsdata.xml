<?xml version="1.0" encoding="UTF-8"?>
<Config xmlns="http://pypi.org/project/xsdata" version="25.7">
    <Output maxLineLength="79" genericCollections="false" unionType="false">
        <Package>generated</Package>
        <Format
                repr="true"
                eq="true"
                order="false"
                unsafeHash="false"
                frozen="false"
                slots="false"
                kwOnly="false"
        >vetis_pydantic</Format>
        <Structure>filenames</Structure>
        <DocstringStyle>Google</DocstringStyle>
        <RelativeImports>false</RelativeImports>
        <CompoundFields
                defaultName="choice"
                useSubstitutionGroups="false"
                forceDefaultName="false"
                maxNameParts="3"
        >false</CompoundFields>
        <WrapperFields>false</WrapperFields>
        <PostponedAnnotations>true</PostponedAnnotations>
        <UnnestClasses>true</UnnestClasses>
        <IgnorePatterns>false</IgnorePatterns>
        <IncludeHeader>false</IncludeHeader>
    </Output>
    <Conventions>
        <ClassName case="pascalCase" safePrefix="type"/>
        <FieldName case="snakeCase" safePrefix="value"/>
        <ConstantName case="screamingSnakeCase" safePrefix="value"/>
        <ModuleName case="snakeCase" safePrefix="mod"/>
        <PackageName case="snakeCase" safePrefix="pkg"/>
    </Conventions>
    <Substitutions>
        <!-- Замена постфикса вложенных типов на постфикс InternalType -->
        <Substitution type="class" search="1$" replace="InternalType"/>
    </Substitutions>
    <!-- Расширения, позволяющие наследовать созданные типы от внутренних классов -->
    <!-- содержат регулярное выражение для имени класса, при совпадении которого -->
    <!-- этот класс унаследует указанный для импорта -->
    <Extensions>
        <!-- Модификация операций отправки в заявочных сервисах-->
        <Extension
                type="class"
                class="^ApplicationManagementServicePortTypeSubmitApplicationRequest$"
                import="vetis_pydantic_types.base.SubmitApplicationRequestOperation"
                prepend="true"
                applyIfDerived="true"
        />
        <!-- Модификация операций получения заявок в заявочных сервисах-->
        <Extension
                type="class"
                class="^ApplicationManagementServicePortTypeReceiveApplicationResult$"
                import="vetis_pydantic_types.base.ReceiveApplicationResultOperation"
                prepend="true"
                applyIfDerived="true"
        />
        <!-- Модификация операций по получению данных из справочных сервисов -->
        <Extension
                type="class"
                class="^(?!.*(?:Input|Output|ApplicationManagement)).*ServicePortType.*$"
                import="vetis_pydantic_types.base.RegistryOperation"
                prepend="true"
                applyIfDerived="true"
        />
        <!-- Модификация типов заявок/запросов -->
        <Extension
                type="class"
                class="^(?!.*Application).*Request$"
                import="vetis_pydantic_types.base.ApplicationRequest"
                prepend="true"
                applyIfDerived="true"
        />
        <!-- Модификация типов ответам по заявкам/запросам -->
        <Extension
                type="class"
                class="^(?!.*Application).*Response$"
                import="vetis_pydantic_types.base.ApplicationResponse"
                prepend="true"
                applyIfDerived="true"
        />
        <!-- Модификация типов, описывающих запрос/ответ -->
        <Extension
                type="class"
                class=".*.*(?:Input|Output)$"
                import="vetis_pydantic_types.base.BaseIOOperation"
                prepend="false"
                applyIfDerived="true"
        />
    </Extensions>
</Config>
